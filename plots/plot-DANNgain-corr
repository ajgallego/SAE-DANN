reset
set encoding utf8 # mandatory



LCases="abcdefghijklmnopqrstuvwxyz"
SCases="ᴀʙᴄᴅᴇғɢʜɪᴊᴋʟᴍɴᴏᴘǫʀsᴛᴜᴠᴡxʏᴢ"

toscchr(c)= c eq ""  ?  ""  :  substr( SCases.c, strstrt(LCases.c, c), strstrt(LCases.c, c) )

texsc(s) = strlen(s) <= 1 ? toscchr(s) : texsc(s[1:strlen(s)/2]).texsc(s[(strlen(s)/2)+1:strlen(s)])

sal_label="Salzinnes"
dib_label="Dibco"
phi_label="Phi"

sal_label=texsc(sal_label)
dib_label=texsc(dib_label)
phi_label=texsc(phi_label)

source_sal_text = "Salzinnes-"
source_dib_text = "Dibco-"
source_phi_text = "Phi-"

source_sal_text=texsc(source_sal_text)
source_dib_text=texsc(source_dib_text)
source_phi_text=texsc(source_phi_text)

set terminal pdf
set output "plot_DANNgain_F1corr.pdf"

set multiplot



set size 1, 1
set yrange [0.0:100.0]
set xrange [0.0:1.0]
set xlabel "Correlation coefficient"
set grid

set key box center bottom

#
# define line styles using explicit rgbcolor names
#

set style func linespoints

set style line 1 lt 5 lc rgb "red" lw 2 dt 4 pt 7 ps .75
set style line 2 lt 5 lc rgb "red" lw 2 dt 4 pt 5 ps .75
set style line 3 lt 5 lc rgb "red" lw 2 dt 4 pt 9 ps .85

set style line 4 lt 5 lc rgb "blue" lw 2 dt 4 pt 7 ps .75
set style line 5 lt 5 lc rgb "blue" lw 2 dt 4 pt 5 ps .75
set style line 6 lt 5 lc rgb "blue" lw 2 dt 4 pt 9 ps .85

set style line 7 lt 5 lc rgb "orange" lw 2 dt 4 pt 7 ps .75
set style line 8 lt 5 lc rgb "orange" lw 2 dt 4 pt 5 ps .75
set style line 9 lt 5 lc rgb "orange" lw 2 dt 4 pt 9 ps .85

set style line 10 lt 5 lc rgb "red" lw 2 dt 4 pt 19 ps .75
set style line 11 lt 5 lc rgb "red" lw 2 dt 4 pt 23 ps .85
set style line 12 lt 5 lc rgb "blue" lw 2 dt 4 pt 6 ps .75
set style line 13 lt 5 lc rgb "blue" lw 2 dt 4 pt 23 ps .85
set style line 14 lt 5 lc rgb "orange" lw 2 dt 4 pt 6 ps .75
set style line 15 lt 5 lc rgb "orange" lw 2 dt 4 pt 19 ps .85


set rmargin 5
set ylabel '{/Helvetica F@_{{/*0.05 &{c}}1}} (%)'
set title ""


set origin 0,0
set size 1,1

