        "+" u 1:(NaN) title " " w dots linecolor rgb "white",\
        'data-f1-corr.txt' using 2:1 with point ls 1 title sal_label,\
        'data-f1-corr.txt' using 4:3 with point ls 2 title dib_label,\
        'data-f1-corr.txt' using 6:5 with point ls 3 title phi_label,\
