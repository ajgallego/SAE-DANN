# Implements a 'textsc' function like in (La)TeX.
# Based on original answer at https://stackoverflow.com/a/54174759/11369382




set arrow 1 from 0.365,44.0 to 0.64,44. nohead lc rgb "purple" lt 1
set arrow 2 from 0.365,23. to 0.64,23. nohead lc rgb "purple" lw 1

set arrow 3 from 0.,95.8 to 1.,95.8 nohead dt "-" lc rgb "red" lw 2
set arrow 4 from 0.,80.8 to 1.,80.8 nohead dt "-" lc rgb "blue" lw 2
set arrow 5 from 0.,84.49 to 1.,84.49 nohead dt "-" lc rgb "orange" lw 2

set label 11 source_sal_text at 0.365, 62.5 left nopoint tc rgb "red"
set label 12 source_dib_text at 0.365, 41.5 left nopoint tc rgb "blue"
set label 13 source_phi_text at 0.365, 20.25 left nopoint tc rgb "orange"

#set arrow 6 from 2.5,90.54 to 32.5, 90.54 nohead dt "." 


